{#
/**
 * @file
 * Preload the non-bold & non-italic fonts for the headings and the body copy.
 *
 * Available variables:
 * - pubsub_path: Returns the path to the pubsub theme.
 */
#}

<link rel="preload" href="{{ pubsub_path }}/fonts/inter/inter-v3-latin-regular.woff2" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ pubsub_path }}/fonts/inter/inter-v3-latin-800.woff2" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ pubsub_path }}/fonts/inter/inter-v3-latin-700.woff2" as="font" type="font/woff2" crossorigin>
